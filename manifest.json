{
  "name": "AI Only Google Search",
  "description": "Only see AI and FAQ results when searching on Google.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*", "*://www.google.ca/search*", "*://google.com/search*", "*://google.ca/search*"],
      "js": ["loader.js"],
      "css": ["loader.css"],
      "run_at": "document_start"
    },
    {
      "matches": ["*://www.google.com/search*", "*://www.google.ca/search*", "*://google.com/search*", "*://google.ca/search*"],
      "js": ["inject.js"],
      "css": ["inject.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage", "activeTab"]
}