{
  "name": "AIF Google Search Results",
  "description": "Not a fan of hyperlinks? View AI and FAQ results first when searching on Google.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/search*",
        "*://www.google.ca/search*",
        "*://www.google.co.uk/search*",
        "*://www.google.com.au/search*",
        "*://www.google.co.in/search*",
        "*://www.google.de/search*",
        "*://www.google.fr/search*",
        "*://www.google.it/search*",
        "*://www.google.es/search*",
        "*://www.google.nl/search*",
        "*://www.google.com.br/search*",
        "*://www.google.co.jp/search*",
        "*://www.google.com.sg/search*",
        "*://www.google.co.kr/search*"
      ],
      "js": ["./js/loader.js"],
      "css": ["./css/loader.css"],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://www.google.com/search*",
        "*://www.google.ca/search*",
        "*://www.google.co.uk/search*",
        "*://www.google.com.au/search*",
        "*://www.google.co.in/search*",
        "*://www.google.de/search*",
        "*://www.google.fr/search*",
        "*://www.google.it/search*",
        "*://www.google.es/search*",
        "*://www.google.nl/search*",
        "*://www.google.com.br/search*",
        "*://www.google.co.jp/search*",
        "*://www.google.com.sg/search*",
        "*://www.google.co.kr/search*"
      ],
      "js": ["./js/inject.js"],
      "css": ["./css/inject.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": "./js/service_worker.js"
  }
}