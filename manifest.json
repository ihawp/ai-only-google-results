{
  "name": "Only AI for Google Search",
  "description": "Only see AI and short-answer results when searching on Google.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*", "*://www.google.ca/search*", "*://google.com/search*", "*://google.ca/search*"],
      "js": ["loader.js"],
      "css": ["loader.css"],
      "run_at": "document_start"
    },
    {
      "matches": ["*://www.google.com/search*", "*://www.google.ca/search*", "*://google.com/search*", "*://google.ca/search*"],
      "js": ["inject2.js"],
      "run_at": "document_end"
    }
  ]
}